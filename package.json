{
  "name": "dynamic.envs",
  "version": "0.0.1",
  "description": "Converts json to .env file",
  "main": "index.js",
  "scripts": {
    "build": "tsc ./src/index.ts --require dotenv/config",
    "dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --quiet --clear --respawn ./src/index.ts --require dotenv/config",
    "test": "jest -c ./jest.unit.json",
    "test:coverage": "jest --coverage -c ./src/jest.unit.json",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "dotenv",
    "env",
    "json",
    "convert",
    "convert json to .env file",
    "dynanic env",
    "dynamic envs ts"
  ],
  "author": "Yuri Lima",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.1",
    "node-cache": "^5.1.2",
    "package": "^1.0.1",
    "package.json": "^2.0.1",
    "save-dev": "^0.0.1-security",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node-dev": "^2.0.0"
  }
}
