{
  "name": "dynamic.envs",
  "version": "1.0.1",
  "description": "Converts json to .env file",
  "author": "Yuri Lima",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "./dist/**/*"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run prepack",
    "dev": "ts-node ./src/example.ts",
    "dep:update": "ncu -u",
    "prepublishOnly": "npm run dep:update",
    "clean": "rm -rf ./dist",
    "build:types": "npx tsc --emitDeclarationOnly --declaration",
    "build:cjs:esm": "npx tsup ./src/index.ts --format cjs,esm --sourcemap",
    "prepack": "npm run clean && npm run build:cjs:esm && npm run build:types",
    "commit": "cz",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "node-cache": "^5.1.2",
    "package": "^1.0.1",
    "package.json": "^0.0.0",
    "save-dev": "^0.0.1-security",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.2.3",
    "@types/uuid": "^8.3.4",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "cz-emoji": "^1.3.2-canary.2",
    "jest": "^29.3.1",
    "npm-check-updates": "^16.4.3",
    "semantic-release": "^19.0.5",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsup": "^6.5.0"
  },
  "homepage": "https://yurilima.uk/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yuri-Lima/JsonToEnv"
  },
  "bugs": {
    "url": "https://github.com/Yuri-Lima/JsonToEnv/issues"
  },
  "release": {
    "branches": [
      "updates",
      "master",
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "emoji": true
    }
  },
  "keywords": [
    "dotenv",
    "env",
    "json",
    "convert",
    "JsonToEnv",
    "convert json to .env file",
    "dynanic env",
    "dynamic envs ts"
  ]
}
